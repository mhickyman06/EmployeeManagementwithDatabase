@model int;

@{
    ViewData["Title"] = "404 Error";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="container">

    <div class="alert alert-danger m-4" >
        <h4>404 Not Found Error</h4><hr/>
        <h4>Employee with id=@Model cannot be found</h4>
    </div>
    <a class="btn btn-lg mt-3 ml-4 btn-outline-success" asp-action="Index">Click here to navigate to the Employee List</a>

  
</div>



@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
